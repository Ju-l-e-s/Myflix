{
  "uid": "pi5-architect-master",
  "title": "Architecture Syst\u00e8me (Pi 5 Master)",
  "panels": [
    {
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "fixedColor": "blue",
            "mode": "fixed"
          }
        }
      },
      "gridPos": {
        "h": 4,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "options": {
        "colorMode": "background",
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "/^ip$/",
          "values": false
        },
        "textMode": "value"
      },
      "targets": [
        {
          "expr": "last_over_time(public_ip_info{type='host'}[5m])",
          "format": "table",
          "legendFormat": "{{ip}}",
          "refId": "A"
        }
      ],
      "title": "IP Publique (Host)",
      "type": "stat"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "fixedColor": "purple",
            "mode": "fixed"
          }
        }
      },
      "gridPos": {
        "h": 4,
        "w": 12,
        "x": 12,
        "y": 0
      },
      "options": {
        "colorMode": "background",
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "/^ip$/",
          "values": false
        },
        "textMode": "name"
      },
      "targets": [
        {
          "expr": "last_over_time(vpn_public_ip_info{ip!='Error'}[5m])",
          "format": "table",
          "legendFormat": "{{ip}}",
          "refId": "A"
        }
      ],
      "title": "IP Torrent & VPN (S\u00e9curis\u00e9)",
      "type": "stat"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "celsius",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "orange",
                "value": 65
              },
              {
                "color": "red",
                "value": 75
              }
            ]
          }
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 4
      },
      "targets": [
        {
          "expr": "node_hwmon_temp_celsius{chip='thermal_thermal_zone0', sensor='temp0'}",
          "legendFormat": "Temp\u00e9rature",
          "refId": "A"
        }
      ],
      "title": "Temp\u00e9rature CPU (Focus)",
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "max": 100,
          "min": 0,
          "unit": "percent",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "orange",
                "value": 70
              },
              {
                "color": "red",
                "value": 85
              }
            ]
          }
        }
      },
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 12,
        "y": 4
      },
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ]
        }
      },
      "targets": [
        {
          "expr": "100 - ((node_memory_MemAvailable_bytes * 100) / node_memory_MemTotal_bytes)",
          "refId": "A"
        }
      ],
      "title": "Saturation RAM",
      "type": "gauge"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "max": 100,
          "min": 0,
          "unit": "percent",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "orange",
                "value": 70
              },
              {
                "color": "red",
                "value": 85
              }
            ]
          }
        }
      },
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 18,
        "y": 4
      },
      "options": {
        "displayMode": "basic",
        "orientation": "horizontal",
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "values": false
        }
      },
      "targets": [
        {
          "expr": "sort_desc(sum(rate(container_cpu_usage_seconds_total{name!=\"\", name!=\"/\", name!=\"netdata\"}[5m])) by (name) * 100)",
          "legendFormat": "{{name}}",
          "refId": "A"
        }
      ],
      "title": "Top CPU (Sorted)",
      "type": "bargauge"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "binBps",
          "custom": {
            "drawStyle": "line",
            "fillOpacity": 15,
            "lineWidth": 2
          }
        }
      },
      "gridPos": {
        "h": 10,
        "w": 24,
        "x": 0,
        "y": 12
      },
      "targets": [
        {
          "expr": "rate(container_network_receive_bytes_total{name='gluetun', interface=~'eth0|tun0'}[5m])",
          "legendFormat": "{{interface}} - Download",
          "refId": "A"
        },
        {
          "expr": "rate(container_network_transmit_bytes_total{name='gluetun', interface=~'eth0|tun0'}[5m])",
          "legendFormat": "{{interface}} - Upload",
          "refId": "B"
        }
      ],
      "title": "Flux R\u00e9seau VPN (eth0 & tun0 Only)",
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "decbytes",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 70
              },
              {
                "color": "orange",
                "value": 80
              },
              {
                "color": "red",
                "value": 90
              }
            ]
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "Saturation (%)"
            },
            "properties": [
              {
                "id": "unit",
                "value": "percent"
              },
              {
                "id": "custom.hideFrom",
                "value": {
                  "viz": true
                }
              }
            ]
          }
        ]
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 22
      },
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "center",
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "/Used|Total|Saturation/",
          "values": false
        },
        "textMode": "value_and_name"
      },
      "targets": [
        {
          "expr": "node_filesystem_size_bytes{mountpoint='/'} - node_filesystem_avail_bytes{mountpoint='/'}",
          "legendFormat": "Utilis\u00e9",
          "refId": "Used"
        },
        {
          "expr": "node_filesystem_size_bytes{mountpoint='/'}",
          "legendFormat": "Total",
          "refId": "Total"
        },
        {
          "expr": "100 - ((node_filesystem_avail_bytes{mountpoint='/'} * 100) / node_filesystem_size_bytes{mountpoint='/'})",
          "legendFormat": "Saturation (%)",
          "refId": "Saturation"
        }
      ],
      "title": "Stockage NVMe (Hot Tier)",
      "type": "stat"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "decbytes",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 70
              },
              {
                "color": "orange",
                "value": 80
              },
              {
                "color": "red",
                "value": 90
              }
            ]
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "Saturation (%)"
            },
            "properties": [
              {
                "id": "unit",
                "value": "percent"
              },
              {
                "id": "custom.hideFrom",
                "value": {
                  "viz": true
                }
              }
            ]
          }
        ]
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 22
      },
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "center",
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "/Used|Total|Saturation/",
          "values": false
        },
        "textMode": "value_and_name"
      },
      "targets": [
        {
          "expr": "node_filesystem_size_bytes{mountpoint='/mnt/externe'} - node_filesystem_avail_bytes{mountpoint='/mnt/externe'}",
          "legendFormat": "Utilis\u00e9",
          "refId": "Used"
        },
        {
          "expr": "node_filesystem_size_bytes{mountpoint='/mnt/externe'}",
          "legendFormat": "Total",
          "refId": "Total"
        },
        {
          "expr": "100 - ((node_filesystem_avail_bytes{mountpoint='/mnt/externe'} * 100) / node_filesystem_size_bytes{mountpoint='/mnt/externe'})",
          "legendFormat": "Saturation (%)",
          "refId": "Saturation"
        }
      ],
      "title": "Stockage HDD (Archive)",
      "type": "stat"
    }
  ],
  "refresh": "30s",
  "schemaVersion": 38,
  "tags": [
    "architect",
    "pi5",
    "master"
  ],
  "timezone": "browser",
  "version": 16
}